{
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "target": "ES2022",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowImportingTsExtensions":true
  },
  "include": [
    "src/.vuepress/**/*.ts",
    "src/.vuepress/**/*.vue"
  ],
  "exclude": [
    "node_modules"
  ]
}
